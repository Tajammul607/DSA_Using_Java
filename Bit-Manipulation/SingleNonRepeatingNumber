class BitManipulation {
  static int findSingleNonRepeatingElement(int[] nums){
       int result =0;
       for(int num : nums){
          result ^= num;
       }
       return result;
  }
    public staic void main(String[] args){
    int[] nums = {2, 3, 5, 4, 5, 3, 4};
        int singleElement = findSingleNonRepeatedElement(nums);
        System.out.println("The single non-repeated element is: " + singleElement);
    }
}
